{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "X509Store": {
    "StoreName": "TrustedPeople",
    "StoreLocation": "CurrentUser",
    "OpenFlags": [ "ReadOnly", "IncludeArchived" ]
  },
  "X509Certificate": {
    "X509FindType": "FindBySerialNumber",
    "FindValue": "serial-number-textual-value-here",
    "ValidOnly": true
  }
}
